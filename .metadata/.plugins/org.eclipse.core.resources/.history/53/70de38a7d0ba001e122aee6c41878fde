package com.Arrays;

public class MultiDimensionArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		String array1[][] = new String[2][2];
		
		array1[0][0]=" Manual Testing ";
		array1[0][1]=" Venkat ";
		array1[1][0]=" Selinium ";
		array1[1][1]=" Srinivas ";
	
		/*
		System.out.println(array1[0][0]);
        System.out.println(array1[0][1]);
        System.out.println(array1[1][0]);
        System.out.println(array1[1][1]);
        
        */
		
		
		for(int row=0;row<=1;row++)
		{
			for(int rowofcell=0;rowofcell<=1;rowofcell++)
			{
				System.out.println(array1[row][rowofcell]);
			}
		}
		
		System.out.println("%%%%%%%1%%%%%%%%%%%%%");
		
	for(int row=0;row<array1.length;row++)
	{
		for(int rowofcell=0;rowofcell<array1.length;rowofcell++)
		{
			System.out.print(array1[row][rowofcell]);
		}
	}
        
	
	
        //this is from you tube video
        System.out.println();
        int x[][] = new int[2][2];
      //we can wite like this also
        //int x[][] ={{1,2}, {3,4}};
        
        x[0][0]=4;
        x[0][1]=3;
        x[1][0]=7;
        x[1][1]=6;
        
        
        for(int row=0;row<=1;row++)
		{
			for(int rowofcell=0;rowofcell<=1;rowofcell++)
			{
				System.out.println(x[row][rowofcell]);
			}
		}
		
        System.out.println();
        
        int a[][] ={{1,2,3}, {4,5,6}};
        int b[][] ={{1,2,3}, {4,5,6}};
        
        int c[][] ={{1,2,3}, {4,5,6}};
        System.out.println(a[0][0]);
        for(int i=0;i<2;i++) {
        	for(int j=0;j<3;j++) {
        		c[i][j]=a[i][j]+b[i][j];
        		System.out.print(c[i][j]+" ");
	      }
        	System.out.println();
        }
        
	}

}
